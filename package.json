{
  "name": "rag-chatbot-backend",
  "version": "1.0.0",
  "description": "Production-grade RAG-powered chatbot backend with Express, TypeScript, and clean architecture",
  "main": "dist/app.js",
  "scripts": {
    "build": "npm run clean && tsc",
    "start": "node scripts/start-production.js",
    "start:server": "node dist/app.js",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/app.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rm -rf dist",
    "populate": "ts-node -r tsconfig-paths/register src/scripts/populate-db.ts",
    "populate:prod": "node dist/scripts/populate-db.js"
  },
  "keywords": [
    "rag",
    "chatbot",
    "express",
    "typescript",
    "qdrant",
    "gemini"
  ],
  "author": "Voosh Team",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@qdrant/qdrant-js": "^1.15.1",
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.7.0",
    "morgan": "^1.10.1",
    "redis": "^5.8.2",
    "uuid": "^13.0.0",
    "winston": "^3.17.0",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.5.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^9.35.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  }
}
